{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "cypress-mochawesome-reporter"],
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@launch-page/*": ["cypress/pages/*"],
      "@upcoming-page/*": ["cypress/pages/*"],
      "@history-page/*": ["cypress/pages/*"],
      "@dropAllCollections/*": ["db/*"]
     
    }
  },
  "include": ["**/*.ts", "cypress.config.js"]
}
